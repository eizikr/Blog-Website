<%- include("partials/header") -%>

<h1>Compose</h1>
<form action="\compose" method="POST">
    <div class="form-group" >
        <label for="titleText" class="form-lable">Title</label>
        <input type="text" class="form-control" name="titleText">
        <br>
        <label for="postText" class="form-lable">Post</label>
        <textarea name="postText" class="form-control" cols="30" rows="10"></textarea>
    </div>  
    <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>

<%- include("partials/footer") -%>